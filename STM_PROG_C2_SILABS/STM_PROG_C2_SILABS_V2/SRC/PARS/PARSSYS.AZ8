        .MADDBYTE û
         EQU $ZX.R.IX=$
         DW @.0
         EQU $ZX.R.PC=EXTB_ST
         EQU $ZX.R.BC=$-$ZX.R.IX
         MoveMemAZ $ZX.R.IX,EXBSTR,16,$ZX.R.BC
         EQU $ZX.R.BREAK=EXTB_EN
         RunEmulZXne
         MoveMemZA EXBSTR,16,$ZX.R.IX,$ZX.R.BC
         ORG $ZX.R.IX+$ZX.R.BC
        ..
        .MADDBYTE
        ..
        EQU TMP_CAR_ST=0
        EQU TMP_CAR_EN=0

        .MADDCMD_EN ,û
         EQU TMP_CAR_ST=$
         DB 0
         MADDBYTE @.1
         MADDBYTE SYSCMD.EN_OR_LF     ;ENTER DETECT
         MADDBYTE SYSCMD.ID_END,@.0
         EQU TMP_CAR_EN=$
         ORG TMP_CAR_ST
         DB TMP_CAR_EN-TMP_CAR_ST
         ORG TMP_CAR_EN
        ..

