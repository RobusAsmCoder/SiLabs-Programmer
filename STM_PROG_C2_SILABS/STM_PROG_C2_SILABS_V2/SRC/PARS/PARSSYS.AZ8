$PASINCARRAY.SIZE       EQU 0
$PASINCARRAY.ADDS       EQU 0


        .PASHEX.ADDBLK ,û
         MOVEMEMAZ @.0,65536-@.1,16,@.1
         EQU $ZX.R.BC=@.1
         EQU $ZX.R.PC=EXHE_ST
         EQU $ZX.R.BREAK=EXHE_EN
         RunEmulZXne
         EQU $PASINCARRAY.ADDS=$ZX.R.HL
         EQU $PASINCARRAY.SIZE=$ZX.R.BC
        ..

        .PASHEX.RESET û
         EQU $ZX.R.AF=@.0*256
         EQU $ZX.R.PC=EXRE_ST
         EQU $ZX.R.BREAK=EXHE_EN
         RunEmulZXne
         EQU $PASINCARRAY.ADDS=$ZX.R.HL
         EQU $PASINCARRAY.SIZE=$ZX.R.BC
        ..

        .PASHEX.FINISH
         EQU $ZX.R.PC=EXFI_ST
         EQU $ZX.R.BREAK=EXHE_EN
         RunEmulZXne
         EQU $PASINCARRAY.ADDS=$ZX.R.HL
         EQU $PASINCARRAY.SIZE=$ZX.R.BC
        ..

        .HEXHEX.SAVE ,,û
         PASHEX.ADDBLK @.1,@.2
         PASHEX.FINISH
         PASINCARRAY.SAVE.ASAMADDR
         MOVEMEMZA $PASINCARRAY.ADDS,16,0,$PASINCARRAY.SIZE
         SaveBIN @.0,0,$PASINCARRAY.SIZE
         PASINCARRAY.RESORE.ASAMADDR
        ..

        .PASHEX.SAVE ,,û
         PASHEX.RESET 0
         HEXHEX.SAVE @.0,@.1,@.2
        ..

        .CIIHEX.SAVE ,,û
         PASHEX.RESET 1
         HEXHEX.SAVE @.0,@.1,@.2
        ..





